{
  "name": "@zdzz/schema-form",
  "version": "0.0.26",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "unpkg": "dist/index.es.js",
  "files": [
    "dist",
    "lib",
    "es",
    "package.json",
    "README.md",
    "global.d.ts"
  ],
  "scripts": {
    "build": "pnpm run clear && vite build",
    "build:scss": "esno script/buildScss",
    "version:patch": "pnpm version patch",
    "clear": "rm -rf dist es lib",
    "w": "vite build -w",
    "check:type": "vue-tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./dist/*": "./dist/*",
    "./es/*": "./es/*",
    "./lib/*": "./lib/*"
  },
  "dependencies": {
    "@zdzz/hooks": "*",
    "@zdzz/shared": "*"
  },
  "devDependencies": {
  },
  "peerDependencies": {
  }
}
